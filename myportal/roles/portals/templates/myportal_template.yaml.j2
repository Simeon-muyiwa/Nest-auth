apiVersion: portal.mycourse1.com/v1
kind: MyPortal
metadata:
  name: "{{ mytenantname }}"
  namespace: "{{ item.namespace }}"
spec:
  myportals:
    {% for portal in portals %}
    - 
      name: "{{ portal.name }}"
      replicacount: {{ portal.replicacount }}
    {% endfor %}
  mytenantname: "{{ mytenantname }}"